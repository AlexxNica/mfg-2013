z3=(x230 + (incx * (x23A - x230)), y230 + (incx * (y23A - y230)));

% pen positions 

penpos2(dist2 + (incx * (px - dist2)), ang2);
penpos1(dist1 + (incx * (px - dist1)), ang1);
penpos4(dist4 + (incx * (px - dist4)), ang4);
penpos3(dist3 + (incx * (px - dist3)), ang3);

% pen strokes  
penstroke  z1e ... 
 z2e ... 
 z3e ... 
